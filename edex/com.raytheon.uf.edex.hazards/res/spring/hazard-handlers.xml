<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-2.0.xsd
  http://camel.apache.org/schema/spring http://camel.apache.org/schema/spring/camel-spring.xsd">

	<bean id="regionLookupRequestHandler"
		class="com.raytheon.uf.edex.hazards.handlers.RegionLookupRequestHandler" />
	<bean factory-bean="handlerRegistry" factory-method="register">
		<constructor-arg
			value="com.raytheon.uf.common.dataplugin.events.hazards.requests.RegionLookupRequest" />
		<constructor-arg ref="regionLookupRequestHandler" />
	</bean>
	
	<bean id="purgePracticeInteropHandler"
		class="com.raytheon.uf.edex.hazards.interoperability.handlers.PurgePracticeInteropRecordsHandler" />
	<bean factory-bean="handlerRegistry" factory-method="register">
		<constructor-arg
			value="com.raytheon.uf.common.dataplugin.events.hazards.interoperability.requests.PurgePracticeInteropRecordsRequest" />
		<constructor-arg ref="purgePracticeInteropHandler" />
	</bean>
	
	<bean id="activeTableInteropHandler"
		class="com.raytheon.uf.edex.hazards.interop.activetable.ActiveTableInteropRetrieveHandler" />
	<bean factory-bean="handlerRegistry" factory-method="register">
		<constructor-arg
			value="com.raytheon.uf.common.dataplugin.events.hazards.interoperability.requests.VtecInteroperabilityActiveTableRequest" />
		<constructor-arg ref="activeTableInteropHandler" />
	</bean>
</beans>