<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-2.0.xsd
  http://camel.apache.org/schema/spring http://camel.apache.org/schema/spring/camel-spring.xsd">

	<bean id="regionLookupRequestHandler"
		class="com.raytheon.uf.edex.hazards.handlers.RegionLookupRequestHandler" />
	<bean factory-bean="handlerRegistry" factory-method="register">
		<constructor-arg
			value="com.raytheon.uf.common.dataplugin.events.hazards.requests.RegionLookupRequest" />
		<constructor-arg ref="regionLookupRequestHandler" />
	</bean>
</beans>