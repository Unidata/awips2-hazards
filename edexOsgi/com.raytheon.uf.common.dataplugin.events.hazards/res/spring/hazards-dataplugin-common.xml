<beans xmlns="http://www.springframework.org/schema/beans"	
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:util="http://www.springframework.org/schema/util"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-3.1.xsd
		http://www.springframework.org/schema/util http://www.springframework.org/schema/util/spring-util-2.5.xsd">
	
	<bean id="hazardsPluginName" class="java.lang.String">
		<constructor-arg type="java.lang.String" value="hazards" />
	</bean>	
	
	<bean id="hazardDataFactory" class="com.raytheon.uf.common.dataplugin.hazards.dataaccess.HazardGeometryFactory" />
	
	<bean factory-bean="dataAccessRegistry" factory-method="register" 
	depends-on="hazardsPluginName, hazardDataFactory">
		<constructor-arg ref="hazardsPluginName"/>
		<constructor-arg ref="hazardDataFactory"/>
	</bean>	
</beans>